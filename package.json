{
  "name": "@momsfriendlydevco/dotenv",
  "version": "1.2.5",
  "description": "DotEnv with schema, validation, expiring keys and multi-input support",
  "scripts": {
    "lint": "eslint lib",
    "test": "mocha"
  },
  "keywords": [
    "dotenv",
    "env"
  ],
  "type": "module",
  "imports": {
    "#lib/*": "./lib/*.js"
  },
  "exports": {
    ".": "./lib/dotenv.js",
    "./*": "./lib/*.js"
  },
  "main": "lib/parse.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/dotenv.git"
  },
  "author": "Matt Carter <matt@mfdc.biz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/dotenv/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/dotenv#readme",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "dependencies": {
    "@momsfriendlydevco/readable": "^1.2.1",
    "@momsfriendlydevco/template": "^1.1.2",
    "dotenv": "^16.0.3",
    "micromatch": "^4.0.5",
    "timestring": "^6.0.0"
  },
  "devDependencies": {
    "@momsfriendlydevco/es6": "^1.0.0",
    "chai": "^4.3.7",
    "eslint": "^8.28.0",
    "mocha": "^10.1.0",
    "moment": "^2.29.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  }
}
